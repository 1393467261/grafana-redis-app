{
  "author": "Redis Labs",
  "description": "Redis CLI Panel for Grafana",
  "devDependencies": {
    "@grafana/data": "7.1.5",
    "@grafana/runtime": "7.1.5",
    "@grafana/toolkit": "7.1.5",
    "@grafana/ui": "7.1.5",
    "emotion": "10.0.27"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "Apache-2.0",
  "name": "redis-cli-panel",
  "scripts": {
